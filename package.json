{
  "name": "@ax-sh/ts-morph-kit",
  "type": "module",
  "version": "0.30.0",
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "bio": "bunx biome lint --write",
    "format": "bunx biome format --write",
    "check": "bunx biome check",
    "test": "dotenv -- vitest run",
    "test:watch": "vitest",
    "test:snapupdate": "vitest -u",
    "coverage": "vitest run --coverage",
    "gh:pub": "repo npm publish",
    "pub:dry": "nr pub --dry-run",
    "build": "pkgroll --clean-dist --sourcemap",
    "clean": "rimraf dist",
    "release": "dotenv -v DEBUG=1 release-it -- minor --no-git.push --no-git.requireUpstream --ci",
    "changelog:latest": "nr git-cliff -l",
    "upp": "bunx npm-check-updates -i",
    "ox": "nr oxlint",
    "knip": "knip",
    "act": "act --container-architecture linux/amd64",
    "act:debug": "nr act --env NODE_ENV=development --env DEBUG=true",
    "renovate": "nr act --env RENOVATE_TOKEN=$(gh auth token) -j renovate",
    "renovate:validate": "bunx --yes --package renovate -- renovate-config-validator   "
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "jsonc-parser": "^3.3.1",
    "ts-morph": "^27.0.2",
    "type-fest": "^5.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@biomejs/biome": "2.3.5",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/bun": "^1.3.2",
    "@types/node": "^24.10.1",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.0.2",
    "eslint-plugin-only-warn": "^1.1.0",
    "git-cliff": "^2.10.1",
    "is-ci": "^4.1.0",
    "knip": "^5.69.0",
    "oxlint": "^1.28.0",
    "pkgroll": "^2.20.1",
    "release-it": "^19.0.6",
    "rimraf": "^6.1.0",
    "vitest": "^4.0.8"
  }
}
