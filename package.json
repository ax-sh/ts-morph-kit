{
  "name": "@ax-sh/ts-morph-kit",
  "type": "module",
  "version": "0.28.0",
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "bio": "bunx biome lint --write",
    "format": "bunx biome format --write",
    "check": "bunx biome check",
    "test": "dotenv -- vitest run",
    "test:watch": "vitest",
    "test:snapupdate": "vitest -u",
    "coverage": "vitest run --coverage",
    "gh:pub": "repo npm publish",
    "pub:dry": "nr pub --dry-run",
    "build": "pkgroll --clean-dist --sourcemap",
    "clean": "rimraf dist",
    "release": "dotenv -v DEBUG=1 release-it -- minor --no-git.push --no-git.requireUpstream --ci",
    "changelog:latest": "nr git-cliff -l",
    "upp": "bunx npm-check-updates -i",
    "ox": "nr oxlint",
    "knip": "knip",
    "act": "act --container-architecture linux/amd64",
    "act:debug": "nr act --env NODE_ENV=development --env DEBUG=true",
    "renovate": "nr act --env RENOVATE_TOKEN=$(gh auth token) -j renovate"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "jsonc-parser": "^3.3.1",
    "ts-morph": "^25.0.1",
    "type-fest": "^4.40.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@biomejs/biome": "1.9.4",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/bun": "^1.2.10",
    "@types/node": "^22.14.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.25.1",
    "eslint-plugin-format": "^1.0.1",
    "git-cliff": "^2.8.0",
    "is-ci": "^4.1.0",
    "knip": "^5.50.5",
    "oxlint": "^0.16.7",
    "pkgroll": "^2.12.1",
    "release-it": "^19.0.1",
    "renovate": "^40.0.0",
    "rimraf": "^6.0.1",
    "vitest": "^3.1.2"
  }
}
