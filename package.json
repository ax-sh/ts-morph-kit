{
  "name": "@ax-sh/ts-morph-kit",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    },
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@release-it/conventional-changelog": "^10.0.0",
    "@types/bun": "latest",
    "dotenv-cli": "^8.0.0",
    "git-cliff": "^2.8.0",
    "is-ci": "^4.1.0",
    "oxlint": "^0.16.0",
    "pkgroll": "^2.8.2",
    "release-it": "^18.1.2",
    "rimraf": "^6.0.1",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "lint": "bunx biome lint --write",
    "format": "bunx biome format --write",
    "check": "bunx biome check",
    "test": "dotenv -- vitest run",
    "test:watch": "vitest",
    "test:snapupdate": "vitest -u",
    "coverage": "vitest run --coverage",
    "pub": "nr prepublishOnly && npm publish --scope=@ax-sh --registry=https://npm.pkg.github.com",
    "gh:pub": "NPM_CONFIG_REGISTRY=\"https://npm.pkg.github.com/\" npm publish --//npm.pkg.github.com/:_authToken=\"$(gh auth token)\"",
    "pub:dry": "nr pub --dry-run",
    "gh:publish": "repo npm publish",
    "clean": "rimraf dist",
    "build": "pkgroll",
    "release": "dotenv -v DEBUG=1 release-it -- minor --no-git.push --no-git.requireUpstream --ci",
    "changelog:latest": "nr git-cliff -l"
  },
  "dependencies": {
    "ts-morph": "^25.0.1"
  },
  "version": "0.4.0"
}
